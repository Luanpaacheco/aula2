#include <stdio.h>
int main()
{
    int valor = 0.0;
    printf("digite um numero: ");
    scanf("%d", & valor);

    if (valor%2 ==0 ){ 
        printf ("par");
    } else{
        printf("impar");
        if (valor %3==0 || valor % 5 ==0 || valor % 7 == 0  )
        {
            printf("valor eh multiplo de 3, 5 ou 7");
        } 
      
        

    } 
   
    
}
